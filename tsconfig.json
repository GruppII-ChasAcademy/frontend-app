{
  "extends": "expo/tsconfig.base",
   "compilerOptions": {
    "jsx": "react-jsx",
    "strict": true,
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "bundler", 
    "esModuleInterop": true,
    "skipLibCheck": true,
    "isolatedModules": true,
    "noEmit": true
  },
  "include": ["./"]
}